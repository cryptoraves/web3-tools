{
  "name": "cryptoraves-tools",
  "description": "web3 front and backend",
  "version": "0.0.0",
  "scripts": {
    "compile": "specron compile",
    "sandbox": "specron sandbox",
    "prepare": "specron compile",
    "test": "specron compile && specron test"
  },
  "specron": {
    "compiler": {
      "build": "./build",
      "match": [
        "./src/**/*.sol"
      ]
    },
    "test": {
      "match": [
        "./src/**/*.test.*"
      ]
    },
    "require": [
      "ts-node/register"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@specron/cli": "latest",
    "@specron/spec": "latest",
    "assemblyscript": "^0.10.0",
    "solc": "0.6.1",
    "ts-node": "8.4.1",
    "typescript": "3.6.3",
    "web3": "1.2.0"
  }
}
